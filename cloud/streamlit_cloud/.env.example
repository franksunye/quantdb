# Streamlit Cloud Environment Configuration
# Copy this file to .env and modify as needed

# Force read-only mode for database operations
QUANTDB_READONLY_MODE=true

# Streamlit Cloud specific settings
STREAMLIT_SHARING_MODE=true

# Database configuration (read-only)
DATABASE_URL=sqlite:///database/quantdb.db

# Logging level
LOG_LEVEL=INFO

# API configuration
API_TIMEOUT=30
MAX_RETRIES=3

# AKShare configuration
AKSHARE_TIMEOUT=30
AKSHARE_MAX_RETRIES=2
